#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_lig_e46d34cd52ba7115c07a9244e75d09f8/node_modules/astro/node_modules:/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_lig_e46d34cd52ba7115c07a9244e75d09f8/node_modules:/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_lig_e46d34cd52ba7115c07a9244e75d09f8/node_modules/astro/node_modules:/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/astro@5.17.1_jiti@2.6.1_lig_e46d34cd52ba7115c07a9244e75d09f8/node_modules:/mnt/c/Users/user/Desktop/royal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
